{
  "name": "cassie/common",
  "type": "library",
  "autoload" : {
    "psr-4" : {
      "App\\" : "src/App/"
    }
  },
  "require": {
    "php": "^8.3",
    "emreyarligan/enum-concern": "^1.0",
    "wendelladriel/laravel-validated-dto": "^3.8"
  },
  "config" : {
    "allow-plugins" : {
      "pestphp/pest-plugin" : true,
      "php-http/discovery" : true
    },
    "optimize-autoloader" : true,
    "preferred-install" : "dist",
    "sort-packages" : true
  },
  "require-dev": {
    "pestphp/pest": "^3.5"
  }
}
